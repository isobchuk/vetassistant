{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Vetassistant",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/output/debug/bin/vetassistant",
            "args": ["--qml-file", "/src/gui/qt/qml/ui.qml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}